# Main Marco's configuration file

version = "1.0"

robot:
{
	name = "Marco"
	
	# Numbers of the pins have to be in BCM format.
	# eg.: The pin P1_12 will be 18
	
	tracks:
	{
		left:
		{
			pinA = 15;
			pinB = 14;
			pinE = 18;
		}
		
		right:
		{
			pinA = 8;
			pinB = 7;
			pinE = 25;
		}
	}
	
	web:
	{
		port = 9002;
		logs = false;
	}
};
